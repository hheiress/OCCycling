        .catch((error) => {\n                        console.error('Error:', error);\n                    });*/\n\n            setTimeout(() => {\n\n                alert(\"User Updated!\");\n                setSubmitting(false);\n\n\n            }, 500);\n        }\n    }\n\n \n\n    const handleChange = event => {\n        setUser({\n            name: event.target.name,\n            value: event.target.value,\n        });\n    }\n\n    const changeHandler = event => {\n        setSelectedFile(event.target.files[0]);\n        setIsFilePicked(true);\n\n        setPhotoThumbnail(<img className=\"photo-btn\" src={URL.createObjectURL(event.target.files[0])} />);\n        console.log(selectedFile);\n\n    }\n\n    const hiddenFileInput = React.useRef(null);\n    const imageUpload = event => {\n        hiddenFileInput.current.click();\n    };\n\n    return (\n        <div>\n            <VolunteerPanel />\n\n            <div className=\"wrapper\">\n                <div className=\"return-form\">\n                    <Link to={'/residents'}>\n                        <p>&#60; ALL RENTERS</p>\n                    </Link>\n                </div>\n\n\n                <div className=\"newrenter-form-wrapper\">\n\n                    <p><b>UPDATE RENTER</b></p>\n\n                    <Form className=\"form-align\" onSubmit={handleSubmit}>\n\n                        <div className=\"margin-form\">\n\n                            <button\n                                type=\"button\"\n                                onClick={imageUpload}\n                                className=\"photo-btn\">{photoThumbnail}</button>\n\n                            <Form.File\n                                id=\"userphoto\"\n                                name=\"user_photo\"\n                                onChange={changeHandler}\n                                value={user.user_photo || ''}\n                                ref={hiddenFileInput}\n                                style={{ display: 'none' }}\n                            />\n                        </div>\n\n                        <div className=\"wrap-names-renter\">\n\n                            <div className=\"margin-form-name\">\n\n                                <Form.Control\n                                    name=\"name\"\n                                    autocomplete=\"off\"\n                                    onChange={handleChange}\n                                    value={user.name}\n                                    placeholder=\"Update Name\"\n                                    disabled={submitting}\n                                    required\n\n                                />\n                            </div>\n\n                            <div className=\"margin-form-name\">\n                                <Form.Control\n                                    name=\"last_name\"\n                                    autocomplete=\"off\"\n                                    onChange={handleChange}\n                                    value={user.last_name}\n                                    placeholder=\"Update Last Name\"\n                                    disabled={submitting}\n                                    required\n\n                                />\n                            </div>\n\n                        </div>\n\n                        <div className=\"margin-form\">\n                            <Form.Control\n                                \n                                name=\"passport\"\n                                autocomplete=\"off\"\n                                onChange={handleChange}\n                                value={user.passport}\n                                placeholder=\"Passport\"\n                                disabled={submitting}\n                                required\n                            />\n                        </div>\n\n                        <div className=\"margin-form\">\n     